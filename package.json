  },
  "homepage": "https://github.com/dylanyoung-dev/careeramplify#readme",
  "devDependencies": {
    "prettier": "^3.3.0",
    "prisma": "^5.15.0",
    "tsx": "^4.11.2",
    "turbo": "^2.0.1"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0"
  }
}
